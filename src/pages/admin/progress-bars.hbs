---
title: Progress bars
subheading: Provide feedback on the progress of a workflow or action.
---

<!-- jQueryUI Progress Bar -->

<!--<link rel="stylesheet" type="text/css" href="../../assets/widgets/progressbar-ui/progressbar.css">
<script type="text/javascript" src="../../assets/widgets/progressbar-ui/progressbar.js"></script>-->

<div id="page-title">
    <h2>{{ title }}</h2>
    <p>{{ subheading }}</p>
    {{> page-title-content }}
</div>

<div class="row">
    <div class="col-md-6">
        <div class="panel">
            <div class="panel-body">
                <h3 class="title-hero">
                    Basic
                </h3>
                <div class="example-box-wrapper">
                    <div class="progressbar-small progressbar" data-value="75">
                        <div class="progressbar-value bg-red tooltip-button" title="75%">
                        </div>
                    </div>
                    <div class="progressbar" data-value="45">
                        <div class="progressbar-value bg-green">
                            <div class="progress-label">45%</div>
                        </div>
                    </div>
                    <div class="progressbar-small progressbar" data-value="25">
                        <div class="progressbar-value bg-blue tooltip-button" title="25%">
                        </div>
                    </div>
                    <div class="progressbar" data-value="62">
                        <div class="progressbar-value bg-primary">
                            <div class="progress-label">62%</div>
                        </div>
                    </div>
                    <div class="progressbar-small progressbar" data-value="81">
                        <div class="progressbar-value bg-yellow tooltip-button" title="81%">
                        </div>
                    </div>
                    <div class="progressbar" data-value="21">
                        <div class="progressbar-value bg-purple">
                            <div class="progress-label">21%</div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="panel">
            <div class="panel-body">
                <h3 class="title-hero">
                    Animated overlay
                </h3>
                <div class="example-box-wrapper">
                    <div class="progressbar-small progressbar" data-value="75">
                        <div class="progressbar-value bg-red tooltip-button" title="75%">
                            <div class="progress-overlay"></div>
                        </div>
                    </div>
                    <div class="progressbar" data-value="45">
                        <div class="progressbar-value bg-green">
                            <div class="progress-overlay"></div>
                            <div class="progress-label">45%</div>
                        </div>
                    </div>
                    <div class="progressbar-small progressbar" data-value="25">
                        <div class="progressbar-value bg-blue tooltip-button" title="25%">
                            <div class="progress-overlay"></div>
                        </div>
                    </div>
                    <div class="progressbar" data-value="62">
                        <div class="progressbar-value bg-primary">
                            <div class="progress-overlay"></div>
                            <div class="progress-label">62%</div>
                        </div>
                    </div>
                    <div class="progressbar-small progressbar" data-value="81">
                        <div class="progressbar-value bg-yellow tooltip-button" title="81%">
                            <div class="progress-overlay"></div>
                        </div>
                    </div>
                    <div class="progressbar" data-value="21">
                        <div class="progressbar-value bg-purple">
                            <div class="progress-overlay"></div>
                            <div class="progress-label">21%</div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="panel">
            <div class="panel-body">
                <h3 class="title-hero">
                    Bootstrap Progress Bars
                </h3>
                <div class="example-box-wrapper">
                    <div class="progress">
                        <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width: 40%">
                            <span class="sr-only">40% Complete (success)</span>
                        </div>
                    </div>
                    <div class="progress">
                        <div class="progress-bar progress-bar-info" role="progressbar" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100" style="width: 20%">
                            <span class="sr-only">20% Complete</span>
                        </div>
                    </div>
                    <div class="progress">
                        <div class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 60%">
                            <span class="sr-only">60% Complete (warning)</span>
                        </div>
                    </div>
                    <div class="progress">
                        <div class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="80" aria-valuemin="0" aria-valuemax="100" style="width: 80%">
                            <span class="sr-only">80% Complete (danger)</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="panel">
            <div class="panel-body">
                <h3 class="title-hero">
                    Stacked progress bars
                </h3>
                <div class="example-box-wrapper">
                    <div class="progress">
                        <div class="progress-bar progress-bar-success" style="width: 35%">
                            <span class="sr-only">35% Complete (success)</span>
                        </div>
                        <div class="progress-bar progress-bar-warning progress-bar-striped" style="width: 20%">
                            <span class="sr-only">20% Complete (warning)</span>
                        </div>
                        <div class="progress-bar progress-bar-danger" style="width: 10%">
                            <span class="sr-only">10% Complete (danger)</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="panel">
            <div class="panel-body">
                <h3 class="title-hero">
                    Striped progress bars
                </h3>
                <div class="example-box-wrapper">
                    <div class="progress progress-striped">
                        <div class="progress-bar bg-blue" style="width: 35%">
                            <span class="sr-only">35% Complete (success)</span>
                        </div>
                        <div class="progress-bar bg-green" style="width: 20%">
                            <span class="sr-only">20% Complete (warning)</span>
                        </div>
                        <div class="progress-bar bg-purple" style="width: 10%">
                            <span class="sr-only">10% Complete (danger)</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>